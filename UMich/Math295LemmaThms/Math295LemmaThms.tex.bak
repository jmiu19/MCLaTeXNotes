		\begin{enumerate}[topsep=3pt,itemsep=-1ex,partopsep=1ex,parsep=1ex]
			\item $\varphi(x +_1 y)=\varphi(x)+_2 \varphi(y)$
			\item $\varphi(x \ast_1 y)=\varphi(x)\ast_2 \varphi(y)$
			\item $\varphi(1_1)=1_2$
		\end{enumerate}
	\end{defn}
	
	\begin{defn}
